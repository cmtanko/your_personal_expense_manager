{
  "name": "personal-expense-manager",
  "version": "1.1.1",
  "private": true,
  "scripts": {
    "start": "react-native start",
    "start:reset": "react-native start --reset-cache",
    "ios": "react-native run-ios",
    "ios:device": "react-native run-ios --device",
    "ios:release": "react-native run-ios --configuration Release",
    "ios:pod": "cd ios && pod install && cd ..",
    "ios:pod-update": "cd ios && pod update && pod install && cd ..",
    "android": "react-native run-android",
    "android:release": "cd android && ./gradlew assembleRelease && cd ..",
    "android:clean": "cd android && ./gradlew clean && cd ..",
    "android:install": "cd android && bundle install && cd ..",
    "build:ios": "react-native bundle --minify --entry-file index.js --platform ios --dev false --bundle-output ./ios/main.jsbundle --assets-dest ./ios/",
    "build:android": "cd android && ./gradlew assembleRelease && cd ..",
    "build:clean": "npm run android:clean && rm -rf $TMPDIR/metro-cache-* && echo 'Clean complete'",
    "test": "jest",
    "test:e2e": "detox test -c ios.sim.release",
    "test:e2e:android": "detox test -c android.emu.release",
    "test:lint": "eslint .",
    "test:lint:fix": "eslint . --fix",
    "format": "prettier --write '*.js'",
    "format:check": "prettier --check '*.js'",
    "format:all": "prettier --write '**/*.{js,jsx,json}' && eslint . --fix",
    "release:android": "cd android && bundle exec fastlane alpha",
    "release:ios": "cd ios && bundle exec fastlane alpha",
    "postinstall": "cd ios && pod install && cd ..",
    "clean": "watchman watch-del-all && rm -rf $TMPDIR/metro-* $TMPDIR/haste-* node_modules/ios/build android/app/build && npm install"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "engines": {
    "node": ">=14.0.0",
    "npm": ">=6.0.0"
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ],
    "*.json": [
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "@haskkor/react-native-pincode": "^1.22.6",
    "@react-native-community/async-storage": "^1.12.1",
    "@react-native-community/google-signin": "^5.0.0",
    "@react-native-community/masked-view": "^0.1.11",
    "@react-native-community/picker": "^1.8.1",
    "@react-native-community/viewpager": "^5.0.11",
    "@react-navigation/bottom-tabs": "^5.11.15",
    "@react-navigation/drawer": "^5.12.9",
    "@react-navigation/material-bottom-tabs": "^5.3.10",
    "@react-navigation/native": "^5.9.8",
    "@react-navigation/stack": "^5.14.12",
    "@types/styled-components": "^5.1.32",
    "axios": "^0.27.2",
    "convert-csv-to-json": "^1.3.1",
    "eslint-plugin-prettier": "^4.2.1",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "native-base": "^2.13.14",
    "react": "16.13.1",
    "react-native": "0.63.3",
    "react-native-app-intro": "^1.1.5",
    "react-native-bootsplash": "^5.3.1",
    "react-native-carousel-view": "^0.5.1",
    "react-native-datepicker": "^1.7.2",
    "react-native-elements": "^3.4.3",
    "react-native-gesture-handler": "^1.10.3",
    "react-native-get-random-values": "^1.9.0",
    "react-native-google-drive-api-wrapper": "^1.4.0",
    "react-native-image-picker": "^5.6.0",
    "react-native-keychain": "^8.1.3",
    "react-native-modalbox": "^2.0.2",
    "react-native-paper": "^5.11.3",
    "react-native-reanimated": "^2.17.0",
    "react-native-safe-area-context": "^4.7.1",
    "react-native-screens": "^3.25.0",
    "react-native-sqlite-storage": "^6.0.1",
    "react-native-super-grid": "^4.9.0",
    "react-native-svg": "^13.10.0",
    "react-native-svg-uri": "^1.2.3",
    "react-native-tab-view": "^3.5.1",
    "react-native-timeline-flatlist": "^0.7.3",
    "react-native-touch-id": "^4.4.1",
    "react-native-vector-icons": "^10.0.3",
    "react-redux": "^7.2.2",
    "redux": "^4.2.1",
    "redux-actions": "^2.6.5",
    "@redux-devtools/extension": "^3.1.3",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.4.2",
    "reselect": "^4.1.8",
    "rn-app-intro-screen": "^1.0.2",
    "rn-fetch-blob": "^0.12.0",
    "styled-components": "^5.3.11",
    "uuid": "^9.0.1",
    "victory-native": "^36.9.2"
  },
  "devDependencies": {
    "@babel/core": "^7.23.6",
    "@babel/runtime": "^7.23.6",
    "@react-native-community/eslint-config": "^3.2.0",
    "babel-jest": "^29.7.0",
    "detox": "^20.16.0",
    "eslint": "^8.54.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "lint-staged": "^15.1.0",
    "metro-react-native-babel-preset": "^0.80.3",
    "prettier": "^3.1.0",
    "react-native-dotenv": "^3.4.9",
    "react-test-renderer": "16.13.1"
  },
  "jest": {
    "preset": "react-native"
  },
  "detox": {
    "configurations": {
      "android.emu.debug": {
        "binaryPath": "android/app/build/outputs/apk/debug/app-debug.apk",
        "build": "cd android && ./gradlew assembleDebug assembleAndroidTest -DtestBuildType=debug && cd ..",
        "type": "android.emulator",
        "name": "Nexus_4_"
      },
      "android.emu.release": {
        "binaryPath": "android/app/build/outputs/apk/release/app-release.apk",
        "build": "cd android && ./gradlew assembleRelease assembleAndroidTest -DtestBuildType=release && cd ..",
        "type": "android.emulator",
        "name": "Nexus_4"
      },
      "android.wallet.device": {
        "binaryPath": "android/app/build/outputs/apk/debug/app-debug.apk",
        "build": "cd android && ./gradlew assembleDebug assembleAndroidTest -DtestBuildType=debug && cd ..",
        "type": "android.attached",
        "name": "HXT7N18911001686"
      }
    },
    "test-runner": "jest"
  }
}